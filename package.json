{
  "name": "@strt/www",
  "description": "Strateg's website",
  "private": true,
  "version": "1.0.0",
  "author": "Alexander Nanberg <alexander.nanberg@strateg.se>",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "gatsby develop -H 0.0.0.0",
    "dev:lambda": "netlify-lambda serve lambda/src -c lambda/webpack.config.js",
    "build": "npm-run-all build:*",
    "build:client": "gatsby build",
    "build:lambda": "netlify-lambda build lambda/src",
    "start": "gatsby serve",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:scripts": "eslint *.js",
    "lint:styles": "stylelint \"src/**/*.js\"",
    "format": "prettier --write \"**/*.{js,md,html,yaml,yml}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "stylelint",
      "prettier --write",
      "git add"
    ],
    "*.{md,html,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/pro-light-svg-icons": "^5.4.1",
    "@fortawesome/pro-regular-svg-icons": "^5.4.1",
    "@mdx-js/mdx": "1.0.0-rc.0",
    "@mdx-js/react": "1.0.0-rc.0",
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "dayjs": "^1.7.7",
    "focus-trap": "^5.0.0",
    "focus-visible": "^4.1.5",
    "gatsby": "^2.1.0",
    "gatsby-mdx": "^0.5.7",
    "gatsby-plugin-catch-links": "^2.0.3",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-netlify": "^2.0.1",
    "gatsby-plugin-netlify-cache": "^1.0.0",
    "gatsby-plugin-netlify-cms": "^3.0.3",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-subfont": "^1.0.1",
    "gatsby-remark-relative-images": "^0.2.0",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.5",
    "gatsby-transformer-sharp": "^2.1.3",
    "js-cookie": "^2.2.0",
    "matter-js": "^0.14.2",
    "netlify-cms": "2.9.1",
    "nprogress": "^0.2.0",
    "polished": "^3.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.3.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.0",
    "react-spring": "^8.0.0",
    "remark-external-links": "^4.0.0",
    "remark-unwrap-images": "^0.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^4.0.0",
    "styled-reset": "^2.0.0",
    "unist-util-find-before": "^2.0.2",
    "unist-util-remove": "^1.0.1",
    "unist-util-visit-children": "^1.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.8.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.6.1",
    "eslint-config-strateg": "^1.0.1",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^1.1.0",
    "lint-staged": "^8.0.0",
    "netlify-lambda": "^1.4.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-strateg": "^1.8.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
